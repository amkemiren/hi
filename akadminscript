    local heart = INVISIBLE_CHAR ..
      NEWLINE ..
        "â¬œâ¬›â¬›â¬œâ¬›â¬›â¬œ" .. NEWLINE ..
        "â¬›â¬›â¬›â¬›â¬›â¬›â¬›" .. NEWLINE ..
        "â¬›â¬›â¬›â¬›â¬›â¬›â¬›" .. NEWLINE ..
