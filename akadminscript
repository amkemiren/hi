    subtitle.TextXAlignment = Enum.TextXAlignment.Left
    subtitle.Parent = mainFrame
    
    local colors = {
        {name = "Slate Grey", color = Color3.fromRGB(128, 128, 128)},
        {name = "Forest Green", color = Color3.fromRGB(34, 139, 34)},
